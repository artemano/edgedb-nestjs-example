{
	"name": "edgedb-nestjs-example",
	"version": "0.0.1",
	"description": "Simple example of configuring a NestJS application to use EdgeDB (and a bespoke GraphQL API)",
	"private": true,
	"license": "UNLICENSED",
	"scripts": {
		"generate:edgeql": "yarn generate:edgeql:client && yarn generate:edgeql:queries",
		"generate:edgeql:client": "npx @edgedb/generate edgeql-js --output-dir ./generated/edgeql --target cjs --force-overwrite",
		"generate:edgeql:queries": "npx @edgedb/generate queries --file ./generated/edgeql/queries --target cjs --force-overwrite",
		"prebuild": "rimraf dist",
		"build": "nest build",
		"dev": "nodemon",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint \"src/**/*.ts\" --fix"
	},
	"dependencies": {
		"@nestjs/apollo": "^10.1.3",
		"@nestjs/common": "^9.1.6",
		"@nestjs/core": "^9.1.6",
		"@nestjs/graphql": "^10.1.3",
		"@nestjs/platform-express": "^9.1.6",
		"apollo-server-express": "^3.10.3",
		"dotenv-safe": "^8.2.0",
		"edgedb": "^1.0.0",
		"graphql": "^16.6.0",
		"reflect-metadata": "^0.1.13",
		"rimraf": "^3.0.2",
		"rxjs": "^7.5.7"
	},
	"devDependencies": {
		"@edgedb/generate": "^0.0.2",
		"@nestjs/cli": "^9.1.4",
		"@types/dotenv-safe": "^8.1.2",
		"@types/express": "^4.17.14",
		"@types/node": "^18.11.5",
		"@typescript-eslint/eslint-plugin": "^5.41.0",
		"@typescript-eslint/parser": "^5.41.0",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.2.1",
		"nodemon": "^2.0.20",
		"prettier": "^2.7.1",
		"source-map-support": "^0.5.21",
		"ts-loader": "^9.4.1",
		"ts-node": "^10.9.1",
		"tsconfig-paths": "^4.1.0",
		"typescript": "^4.8.4"
	}
}
